use Psy\Configuration;
use Psy\Shell;

require __DIR__ . '/../vendor/autoload.php';

try {
    (new Shell(
        new Configuration(
            require __DIR__ . '/../.psysh.php'
        )
    ))->run();
} catch (Throwable $e) {
    fwrite(STDERR, $e->getMessage(). PHP_EOL);
}

